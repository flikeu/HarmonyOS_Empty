{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "my",
      "srcPath": "./libs/my",
    },
    {
      "name": "common",
      "srcPath": "./libs/common",
    },
    {
      "name": "communicate",
      "srcPath": "./libs/communicate",
    },
    {
      "name": "workspace",
      "srcPath": "./libs/workspace",
    },
    {
      "name": "assistance",
      "srcPath": "./libs/assistance",
    }
  ]
